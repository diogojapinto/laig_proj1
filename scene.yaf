<yaf>

    <!-- bloco de configuracoes globais -->
    <!-- a declaracao dos atributos deste bloco e' obrigatoria -->
    <!-- Valores para draw mode: "fill" , "line", "point" -->
    <!-- Valores para shading: "flat", "gouraud" ) -->
    <!-- Valores para cullface: "none", "back" , "front" ou "both" -->
    <!-- Valores para cullorder : "CCW" ou "CW" -->
    <globals background="0.25 0.73 0.92 1" drawmode="fill" shading="gouraud" cullface="none" cullorder="CCW" />
    
    <!-- bloco de camaras: o atributo "initial" identifica a camara/ponto de vista inicial -->
    <cameras initial="obliqueView" >
        <!-- tem de existir, pelo menos uma camara dos tipos seguintes (perspective ou ortho) -->

        <perspective id="frontView" near="0.0001" far="1000.0" angle="45.0" pos="50.0 15.0 10.0" target="30.0 10.0 0.0" />
        
        <ortho id="obliqueView" near="-10.0" far="100.0" left="-10.0" right="30.0" top="25.0" bottom="-5.0" />

    </cameras>
  

    <!-- bloco de iluminacao; segue os parametros do OpenGL -->
    <lighting doublesided="true" local="true" enabled="true" ambient="0.0 0.0 0.0 0.0" >
             <!-- Deve existir pelo menos um dos seguintes blocos: omin ou spot -->
            <!-- Os identificadores "id" nao podem ser repetidos -->
            <spot id="light0" enabled="true"
                location="5.5 6.0 1.5"
                ambient="0.0 0.0 0.0 1.0"
                diffuse="1.0 1.0 1.0 1.0"
                specular="1.0 1.0 1.0 1.0"
                angle="20.0"
                exponent="20.0"
                direction="1.0 -0.5 1.0"
            />

            <spot id="light1" enabled="true"
                location="14.5 6.0 1.5"
                ambient="0.0 0.0 0.0 1.0"
                diffuse="1.0 1.0 1.0 1.0"
                specular="1.0 1.0 1.0 1.0"
                angle="20.0"
                exponent="20.0"
                direction="-1.0 -0.5 1.0"
            />

            <omni id="light2" enabled="true"
                location="6.5 8.0 22.0"
                ambient="0.0 0.0 0.0 1.0"
                diffuse="1.0 1.0 1.0 1.0" 
                specular="1.0 1.0 1.0 1.0"
            />

            <omni id="light3" enabled="true"
                location="13.5 8.0 22.0"
                ambient="0.0 0.0 0.0 1.0"
                diffuse="1.0 1.0 1.0 1.0" 
                specular="1.0 1.0 1.0 1.0"
            />

            <omni id="light4" enabled="false"
                location="30.0 30.0 30.0"
                ambient="0.0 0.0 0.0 1.0"
                diffuse="1.0 1.0 1.0 1.0" 
                specular="1.0 1.0 1.0 1.0"
            />

    </lighting>

    <!-- bloco de texturas. Este bloco tem de existir mas pode estar vazio, se nao forem usadas texturas -->
    <textures>       
          <texture id="bush" file="bush.jpg" />
          <texture id="door" file="door.jpg" />
          <texture id="grass" file="grass.jpg" />
          <texture id="lamp" file="lamp.jpg" />
          <texture id="post" file="post.jpeg" />
          <texture id="rooftop" file="rooftop.jpg" />
          <texture id="rope" file="rope.png" />
          <texture id="tableleg" file="tableleg.jpg" />
          <texture id="tabletop" file="tabletop.jpg" />
          <texture id="tire" file="tire.jpg" />
          <texture id="tree" file="tree.jpg" />
          <texture id="treetop" file="treetop.jpeg" />
          <texture id="wall" file="wall.jpg" />
    </textures>

    <!-- bloco de materiais; -->
    <appearances>
        <!-- deve existir um ou mais blocos "appearance" -->
        <!-- IMPORTANTE: -->
        <!--   textureref, texlength_s e textlength_t sao atributos -->
        <!--   opcionais se for usada uma textura. -->
        <!--   ver topico 'Notas finais' no enunciado sobre os -->
        <!--   parametros texlength_...) -->
        
        <appearance id="default"
            emissive="0.0 0.0 0.0 0.0"
            ambient="1.0 1.0 1.0 1.0"
            diffuse="1.0 1.0 1.0 1.0"
            specular="1.0 1.0 1.0 1.0"
            shininess="10.0"  />

        <appearance id="house_walls_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.1 0.1 0.1 1.0"
            diffuse="0.1 0.1 0.1 0.0"
            specular="0.1 0.1 0.1 0.0"
            shininess="10.0"
            textureref="wall"
            texlength_s="1"
            texlength_t="1" />

        <appearance id="floor_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.1 0.12 0.11 0.0"
            diffuse="0.1 0.12 0.11 0.0"
            specular="0.1 0.12 0.11 0.0"
            shininess="60"
            textureref="grass"
            texlength_s="5"
            texlength_t="5" />

        <appearance id="fence_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.1 0.12 0.11 0.0"
            diffuse="0.1 0.12 0.11 0.0"
            specular="0.1 0.12 0.11 0.0"
            shininess="60"
            textureref="bush"
            texlength_s="10"
            texlength_t="10" />

        <appearance id="door_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.1 0.1 0.1 0.0"
            diffuse="0.1 0.1 0.1 0.0"
            specular="0.1 0.1 0.1 0.0"
            shininess="10.0"
            textureref="door"
            texlength_s="1"
            texlength_t="1" />

        <appearance id="roof_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.1 0.1 0.1 0.0"
            diffuse="0.1 0.1 0.1 0.0"
            specular="0.1 0.1 0.1 0.0"
            shininess="10.0"
            textureref="rooftop"
            texlength_s="5"
            texlength_t="5" />

        <appearance id="table_top_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.1 0.1 0.1 0.0"
            diffuse="0.2 0.2 0.2 0.0"
            specular="0.4 0.4 0.4 0.0"
            shininess="120.0"
            textureref="tabletop"
            texlength_s="2"
            texlength_t="2" />

        <appearance id="table_leg_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.1 0.1 0.1 0.0"
            diffuse="0.2 0.2 0.2 0.0"
            specular="0.4 0.4 0.4 0.0"
            shininess="120.0"
            textureref="tableleg"
            texlength_s="5"
            texlength_t="5" />

        <appearance id="tree_trunk_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.13 0.13 0.1 0.0"
            diffuse="0.13 0.13 0.1 0.0"
            specular="0.13 0.13 0.1 0.0"
            shininess="10.0"
            textureref="tree"
            texlength_s="3"
            texlength_t="3" />

        <appearance id="tree_leafs_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.1 0.12 0.11 0.0"
            diffuse="0.1 0.12 0.11 0.0"
            specular="0.1 0.12 0.11 0.0"
            shininess="60"
            textureref="treetop"
            texlength_s="2"
            texlength_t="2" />

        <appearance id="rope_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.13 0.13 0.1 0.0"
            diffuse="0.13 0.13 0.1 0.0"
            specular="0.13 0.13 0.1 0.0"
            shininess="10.0"
            textureref="rope"
            texlength_s="5"
            texlength_t="5" />

        <appearance id="swing_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.1 0.1 0.1 0.0"
            diffuse="0.2 0.2 0.2 0.0"
            specular="0.4 0.4 0.4 0.0"
            shininess="120.0"
            textureref="tire"
            texlength_s="5"
            texlength_t="5" />

        <appearance id="light_body_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.1 0.1 0.1 0.0"
            diffuse="0.2 0.2 0.2 0.0"
            specular="0.4 0.4 0.4 0.0"
            shininess="120.0"
            textureref="post"
            texlength_s="5"
            texlength_t="5" />

        <appearance id="light_bulb_appearence"
            emissive="0.0 0.0 0.0 0.0"
            ambient="0.1 0.1 0.1 0.0"
            diffuse="0.2 0.2 0.2 0.0"
            specular="0.4 0.4 0.4 0.0"
            shininess="120.0"
            textureref="lamp"
            texlength_s="2"
            texlength_t="2" />
    </appearances>

   <!-- bloco do grafo; "rootid" identifica o no de raiz -->
    <graph rootid="scene">

        <!-- tem de existir, pelo menos, um bloco "node" -->
        <node id="scene">
            <!-- bloco "transforms" obrigatorio -->
            <transforms>
                <translate to="0.0 7.0 -15.0" />
                <rotate axis="y" angle="30" />
            </transforms>
                              
             <!-- bloco "children" obrigatorio -->
            <children>

                <noderef id="home" />
                <noderef id="garden" />
                <noderef id="street" />

            </children>
        </node>

        <node id="home">
            <!-- bloco "transforms" obrigatorio -->
            <transforms>
            </transforms>
                              
             <!-- bloco "children" obrigatorio -->
            <children>

                <noderef id="wall" />
                <noderef id="door" />
                <noderef id="roof" />

            </children>

        </node>

        <node id="wall">
            <!-- bloco "transforms" obrigatorio -->
