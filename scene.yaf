             <!-- bloco "children" obrigatorio -->
            <children>

                <noderef id="table" />
                <noderef id="tree" />
                <noderef id="grassSquare" />
                
            </children>

        </node>

        <node id="grassSquare">
            <!-- bloco "transforms" obrigatorio -->
