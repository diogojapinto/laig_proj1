<yaf> 

    <globals background="0 0 0 1" drawmode="fill" shading="gouraud" cullface="none" cullorder="CCW" /> 

    <cameras initial="cam1" > 

        <perspective id="cam1" near="0.1" far="1000" angle="60" pos="15 15 15" target="0 0.0 0" /> 

    </cameras> 
   
    <lighting doublesided="false" local="true" enabled="true" ambient="1.0 1.0 1.0 1" > 
	
            <omni id="light1" enabled="true" 
                location="3.75 7.3 3.75" 
                ambient="0.1 0.03 0.03 0.03" 
                diffuse="0.5 0.6 0.8 1" 
                specular="0.2 0.2 0.3 1" 
            /> 

            <omni id="light2" enabled="true" 
                location="11.25 7.3 3.75" 
                ambient="0.1 0.03 0.03 0.03" 
                diffuse="0.5 0.6 0.8 1" 
                specular="0.2 0.2 0.3 1" 
            /> 
			
			<omni id="light3" enabled="false" 
                location="11.25 7.3 3.75" 
                ambient="0.1 0.03 0.03 0.03" 
                diffuse="0.5 0.6 0.8 1" 
                specular="0.2 0.2 0.3 1" 
            /> 
			
			<spot id="light4" enabled="false" 
                location="2 2 4" 
                ambient="0.1 0.2 0.3 0.5" 
                diffuse="0.5 0.6 0.8 1" 
                specular="0.2 0.2 0.3 1" 
                angle="4" 
                exponent="2" 
                direction="5 0.2 1" 
            /> 

    </lighting> 

    <!-- bloco de texturas. Este bloco tem de existir mas pode estar vazio, se nao forem usadas texturas --> 
    <textures>        
        <texture id="leftWallTex" file="scratchedWall.jpg" />
		<texture id="rightWallTex" file="bloodyWall.jpg" />
		<texture id="floorTex" file="test1.jpg" />
		<texture id="tableTex" file="table.png" />
		<texture id="crimeWeaponTex" file="bloodyPrints.jpg" />
		<texture id="tapeTex" file="pTape.jpg" />
		<texture id="tapeSupportTex" file="lamp.png" />
		<texture id="pineTex" file="pine.png" />
		<texture id="vaseTex" file="vase.png" />
    </textures> 

    <appearances> 

        <appearance id="tableApp" 
            emissive="0.5 0.5 0.5 1" 
            ambient="0.1 0.03 0.03 0.03" 
            diffuse="0.8 0.8 0.8 1.0" 
            specular="0 0 0 0" 
            shininess="10" 
            textureref="tableTex" 
            texlength_s="1" 
            texlength_t="1" />
			
		<appearance id="leftWallApp" 
            emissive="0.5 0.5 0.5 1" 
			ambient="0.1 0.03 0.03 0.03" 
            diffuse="0.8 0.8 0.8 1.0" 
            specular="0 0 0 0" 
            shininess="0" 
            textureref="leftWallTex" 
            texlength_s="1" 
            texlength_t="1" />
		
		<appearance id="rightWallApp" 
            emissive="0.5 0.5 0.5 1" 
            ambient="0.1 0.03 0.03 0.03" 
            diffuse="0.8 0.8 0.8 1.0" 
            specular="0 0 0 0" 
            shininess="0" 
            textureref="rightWallTex" 
            texlength_s="1" 
            texlength_t="1" />
		
		<appearance id="floorApp" 
            emissive="0.5 0.5 0.5 1" 
            ambient="0.1 0.03 0.03 0.03" 
            diffuse="0.8 0.8 0.8 1.0"  
            specular="0 0 0 0" 
            shininess="0" 
            textureref="floorTex" 
            texlength_s="1" 
            texlength_t="1" />
		
		<appearance id="crimeWeaponApp" 
            emissive="0.5 0.5 0.5 1" 
            ambient="0.1 0.03 0.03 0.03" 
            diffuse="0.8 0.8 0.8 1.0"  
            specular="0 0 0 0" 
            shininess="0" 
            textureref="crimeWeaponTex" 
            texlength_s="1" 
            texlength_t="1" />
			
		<appearance id="tapeApp" 
            emissive="0.5 0.5 0.5 1" 
            ambient="0.1 0.03 0.03 0.03" 
            diffuse="0.8 0.8 0.8 1.0"  
            specular="0 0 0 0" 
            shininess="0" 
            textureref="tapeTex" 
            texlength_s="1" 
            texlength_t="1" />
			
		<appearance id="tapeSupportApp" 
            emissive="0.5 0.5 0.5 1" 
            ambient="0.1 0.03 0.03 0.03" 
            diffuse="0.8 0.8 0.8 1.0"  
            specular="0 0 0 0" 
            shininess="0" 
            textureref="tapeSupportTex" 
            texlength_s="1" 
            texlength_t="1" />
		
		<appearance id="pineApp" 
            emissive="0.5 0.5 0.5 1" 
            ambient="0.1 0.03 0.03 0.03" 
            diffuse="0.8 0.8 0.8 1.0"  
            specular="0 0 0 0" 
            shininess="0" 
            textureref="pineTex" 
            texlength_s="1" 
            texlength_t="1" />
			
		<appearance id="vaseApp" 
            emissive="0.5 0.5 0.5 1" 
            ambient="0.1 0.03 0.03 0.03" 
            diffuse="0.8 0.8 0.8 1.0"  
            specular="0 0 0 0" 
            shininess="0" 
            textureref="vaseTex" 
            texlength_s="1" 
            texlength_t="1" />
			
    </appearances> 

    <graph rootid="scene"> 
		
		<node id="scene"> 

            <transforms>
            </transforms> 

            <children> 
                <noderef id="leftWall" /> 
				<noderef id="rightWall" /> 
				<noderef id="floor" /> 
				<noderef id="table" /> 
				 
				<noderef id="tree" /> 
				<noderef id="policeWarning1" /> 
				<noderef id="policeWarning2" />
				<noderef id="crimeWeapon" />
            </children> 
        </node>
		
		<node id="rectangle1"> 

            <transforms>
				<translate to="0 0 -0.5" />
				<rotate axis="y" angle="180" />
            </transforms> 

            <children>             
                <rectangle xy1="-0.5 -0.5" xy2="0.5 0.5" /> 
            </children> 
        </node>
		
		<node id="rectangle2"> 

            <transforms>
				<translate to="0 0 0.5" />
            </transforms> 

            <children>             
                <rectangle xy1="-0.5 -0.5" xy2="0.5 0.5" /> 
            </children> 
        </node>
		
		<node id="rectangle3"> 

            <transforms>
				<translate to="0 -0.5 0" />
				<rotate axis="x" angle="90" />
            </transforms> 

            <children>             
                <rectangle xy1="-0.5 -0.5" xy2="0.5 0.5" /> 
            </children> 
        </node>
		
		<node id="rectangle4"> 

            <transforms>
				<translate to="0 0.5 0" />
				<rotate axis="x" angle="-90" />
            </transforms> 

            <children>             
                <rectangle xy1="-0.5 -0.5" xy2="0.5 0.5" /> 
            </children> 
        </node>
		
		<node id="rectangle5"> 

            <transforms>
				<translate to="-0.5 0 0" />
				<rotate axis="y" angle="-90" />
            </transforms> 

            <children>             
                <rectangle xy1="-0.5 -0.5" xy2="0.5 0.5" /> 
            </children> 
        </node>
		
		<node id="rectangle6"> 

            <transforms>
				<translate to="0.5 0 0" />
				<rotate axis="y" angle="90" />
            </transforms> 

            <children>             
                <rectangle xy1="-0.5 -0.5" xy2="0.5 0.5" /> 
            </children> 
        </node>
		
		<node id="unitCube">
			<transforms>
            </transforms> 

            <children>            
				<!-- Back -->
                <noderef id="rectangle1" /> 
				<!-- Front -->
				<noderef id="rectangle2" /> 
				<!-- Bottom -->
				<noderef id="rectangle3" /> 
				<!-- Top -->
				<noderef id="rectangle4" />
				<!-- Left -->				
				<noderef id="rectangle5" />
				 <!-- Right -->
				<noderef id="rectangle6" />
            </children> 
        </node>
		
		<node id="leg">
			<transforms>
				<scale factor="0.06 0.92 0.06" />
            </transforms> 

            <children>             
                <noderef id="unitCube" />
            </children> 
        </node>
		
		<node id="leg1">
			<transforms>
				<translate to="0.45 0 0.42" />
            </transforms> 

            <children>             
                <noderef id="leg" />
            </children> 
        </node>
		
		<node id="leg2">
			<transforms>
				<translate to="0.45 0 -0.42" />
            </transforms> 

            <children>             
                <noderef id="leg" />
            </children> 
        </node>
		
		<node id="leg3">
			<transforms>
				<translate to="-0.45 0 0.42" />
            </transforms> 

            <children>             
                <noderef id="leg" />
            </children> 
        </node>
		
		<node id="leg4">
			<transforms>
				<translate to="-0.45 0 -0.42" />
            </transforms> 

            <children>             
                <noderef id="leg" />
            </children> 
        </node>
		
		<node id="tableTop">
			<transforms>
				<translate to="0 0.48 0" />
				<scale factor="1 0.08 1" />
            </transforms> 

            <children>             
                <noderef id="unitCube" />
            </children> 
        </node>
		
		<node id="table">
			<transforms>
				<translate to="7.5 0 10" />
				<translate to="0 1.5 0" />
				<rotate axis="x" angle="89.5" />
				<scale factor="4.5 2.5 3" />
				<translate to="0 0.5 0" />
            </transforms> 

			<appearanceref id="tableApp" />
			
            <children>             
                <noderef id="tableTop" />
				<noderef id="leg1" />
				<noderef id="leg2" />
				<noderef id="leg3" />
				<noderef id="leg4" />
            </children> 
        </node>
		
		
		<node id="floor">
			<transforms>
				<translate to="0 0 15" />
				<rotate axis="x" angle="-90" />
            </transforms> 

			<appearanceref id="floorApp" />
			
            <children>             
                <rectangle xy1="0 0" xy2="15 15" />
            </children> 
        </node>
		
		<node id="wall">
			<transforms>
				<rotate axis="x" angle="-90" />
            </transforms> 
			
            <children>             
                <noderef id="rectangle1" />
            </children> 
        </node>
		
		<node id="leftWall">
			<transforms>
				<translate to="0 0 15" />
				<rotate axis="y" angle="90" />
            </transforms> 

			<appearanceref id="leftWallApp" />
			
            <children>             
                <rectangle xy1="0 0" xy2="15 8" />
            </children> 
        </node>
		
		<node id="rightWall">
			<transforms />
			
			<appearanceref id="rightWallApp" />
			
            <children>             
                <rectangle xy1="0 0" xy2="15 8" />
            </children> 
        </node>
		
		<node id="crimeWeapon">
			<transforms>
				<translate to="7 0.5 8" /> 
				<scale factor="0.5 0.5 5" />
            </transforms> 

			<appearanceref id="crimeWeaponApp" />
			
            <children>             
                <noderef id="unitCube" />
            </children> 
        </node>
		
		<node id="support">
			<transforms />

            <children>             
                <cylinder base="0.2" top="0.2" height="3" slices="32" stacks="32" />
            </children> 
        </node>
		
		<node id="tapeSupport1">
			<transforms>
				<rotate axis="x" angle="-90"/>
				<translate to="-6 0 0" /> 
            </transforms> 
			
			<appearanceref id="tapeSupportApp" />
			
            <children>             
                <noderef id="support" />
            </children> 
        </node>
		
		<node id="tapeSupport2">
			<transforms>
				<rotate axis="x" angle="-90"/>
				<translate to="6 0 0" /> 
            </transforms> 
			
			<appearanceref id="tapeSupportApp" />
			
            <children>             
                <noderef id="support" />
            </children> 
        </node>
		
		<node id="tape">
			<transforms>
				<translate to="0 2.6 0" />
            </transforms> 
			
			<appearanceref id="tapeApp" />
			
            <children>             
                <rectangle xy1="-6 -0.2" xy2="6 0.2" />
            </children> 
        </node>
		
		<node id="policeWarning">
			<transforms />
			
			<children>             
                <noderef id="tape" />
				<noderef id="tapeSupport1" />
				<noderef id="tapeSupport2" />
            </children> 
		</node>
		
		<node id="policeWarning1">
			<transforms>
				<translate to="7.5 0 14" />
            </transforms> 
			
            <children>             
                <noderef id="policeWarning" />
            </children> 
        </node>
		
		<node id="policeWarning2">
			<transforms>
				<translate to="14 0 7.5" />
				<rotate axis="y" angle="90"/>
            </transforms> 
			
            <children>             
                <noderef id="policeWarning" />
            </children> 
        </node>
		
		<node id="vase">
			<transforms>
				<rotate axis="x" angle="-90"/>
            </transforms> 
			
			<appearanceref id="vaseApp" />
			
            <children>             
                <cylinder base="0.75" top="1.3" height="1.9" slices="32" stacks="32" />
            </children> 
        </node>
		
		<node id="trunk">
			<transforms>
				<translate to="0 1.7 0" />
				<rotate axis="x" angle="-90"/>
            </transforms> 
			
			<appearanceref id="tableApp" />
			
            <children>             
                <cylinder base="0.4" top="0.4" height="1" slices="32" stacks="32" />
            </children> 
        </node>
		
		<node id="treeSeg1">
			<transforms>
				<translate to="0 2.7 0" />
				<rotate axis="x" angle="-90"/>	
            </transforms> 
			
			<appearanceref id="pineApp" />
			
            <children>             
                <cylinder base="2" top="1" height="1.9" slices="32" stacks="32" />
            </children> 
        </node>
		
		<node id="treeSeg2">
			<transforms>
				<translate to="0 4.6 0" />
				<rotate axis="x" angle="-90"/>
            </transforms> 
			
			<appearanceref id="pineApp" />
			
            <children>             
                <cylinder base="1.5" top="0.75" height="2" slices="32" stacks="32" />
            </children> 
        </node>
		
		<node id="treeSeg3">
			<transforms>
				<translate to="0 6.6 0" />
				<rotate axis="x" angle="-90"/>
            </transforms> 
			
			<appearanceref id="pineApp" />
			
            <children>             
                <cylinder base="0.9" top="0" height="2" slices="32" stacks="32" />
            </children> 
        </node>
		
		<node id="tree">
			<transforms>
				<translate to="2 0.7 2" />
				<rotate axis="z" angle="-80"/>
            </transforms> 
			
            <children>             
                <noderef id="vase" />
				<noderef id="trunk" />
				<noderef id="treeSeg1" />
				<noderef id="treeSeg2" />
				<noderef id="treeSeg3" />
            </children> 
        </node>
		
    </graph> 
</yaf> 